def sum_numbers(n_1, n_2, n_3):
    return n_1 + n_2

def substract(n_1, n_2, n_3):
    result = sum_numbers(n_1, n_2, n_3)
    return result - n_3

def add_and_substract(n_1, n_2, n_3):
    return substract(n_1, n_2, n_3)

num_1 = int(input())
num_2 = int(input())
num_3 = int(input())

print(add_and_substract(num_1, num_2, num_3))
