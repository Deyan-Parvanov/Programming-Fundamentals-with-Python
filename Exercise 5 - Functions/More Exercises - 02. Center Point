from math import floor

def coordinates(a, b):
    result = abs(a) + abs(b)
    return floor(result)

x1 = float(input())
y1 = float(input())

x2 = float(input())
y2 = float(input())

a_coordinate = coordinates(x1, y1)
b_coordinate = coordinates(x2, y2)
if a_coordinate >= b_coordinate:
    print(f"({floor(x2)}, {floor(y2)})")

else:
    print(f"({floor(x1)}, {floor(y1)})")
